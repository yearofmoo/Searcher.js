src:
  input:
    - Source/files/Searcher.Licence.txt
    - Source/files/Searcher.Base.js
    - Source/files/Searcher.Locale.js
    - Source/files/Searcher.AutoComplete.js
    - Source/files/Searcher.Local.js
    - Source/files/Searcher.Local.Filterer.js
    - Source/files/Searcher.Local.Spotlight.js
    - Source/files/Searcher.Loaders.js
  output:
    - Source/Searcher.js

min:
  filters:
    - cat Source/Searcher.js | jsmin > %{output}
  input:
    - Source/Searcher.js
  output:
    - Source/Searcher.min.js
